{
  "emulators": {
    "dataconnect": {
      "dataDir": "dataconnect/.dataconnect/pgliteData"
    }
  },
  "dataconnect": {
    "source": "dataconnect"
  },
  "firestore": {
    "database": "(default)",
    "location": "asia-east1",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "upscmainsweb",
          "appId": "1:1047947149820:android:a2ba310cf73bef16787279",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "upscmainsweb",
          "configurations": {
            "android": "1:1047947149820:android:a2ba310cf73bef16787279",
            "ios": "1:1047947149820:ios:503be7fa2c861d99787279",
            "macos": "1:1047947149820:ios:503be7fa2c861d99787279",
            "web": "1:1047947149820:web:2eb5d7441a228f1e787279",
            "windows": "1:1047947149820:web:876b01cdd2dcafe6787279"
          }
        }
      }
    }
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": []
    },
    {
      "source": "upscmains",
      "codebase": "upscmains",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": []
    }
  ]
